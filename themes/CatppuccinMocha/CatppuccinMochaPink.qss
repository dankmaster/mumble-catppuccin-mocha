@charset "UTF-8";
/*
 * Mumble Catppuccin Mocha (Pink Accent) Theme – Firefox‑aligned Variant
 * Now based on Dark upstream definitions + Firefox theme JSON variable mapping.
 * This entrypoint imports vendor Dark definitions, then overrides variables
 * using colors from Imports/catppuccinfirefox-pink.json to keep changes token-level.
 * Build (from source/):
 *   sass --style=expanded CatppuccinMochaPink.scss ../CatppuccinMochaPink.qss
 */
/*
 * YOU SHOULD NOT MODIFY THIS FILE
 * Edit the files in the "source" folder instead.
 * See project README
 *
 */
ApplicationPalette {
  qproperty-window: #1e1e2e;
  qproperty-windowtext: #cdd6f4;
  qproperty-windowtext_disabled: #6c7086;
  qproperty-base: #181825;
  qproperty-alternatebase: #313244;
  qproperty-text: #cdd6f4;
  qproperty-text_disabled: #6c7086;
  qproperty-tooltipbase: #181825;
  qproperty-tooltiptext: #cdd6f4;
  qproperty-tooltiptext_disabled: #6c7086;
  qproperty-brighttext: #cdd6f4;
  qproperty-brighttext_disabled: #6c7086;
  qproperty-highlight: rgba(147, 153, 178, 0.22);
  qproperty-highlightedtext: #cdd6f4;
  qproperty-highlightedtext_disabled: #6c7086;
  qproperty-button: #313244;
  qproperty-buttontext: #cdd6f4;
  qproperty-buttontext_disabled: #6c7086;
  qproperty-link: #f5c2e7;
  qproperty-linkvisited: #f5c2e7;
  qproperty-light: #1e1e2e;
  qproperty-midlight: transparent;
  qproperty-mid: #1e1e2e;
  qproperty-dark: transparent;
  qproperty-shadow: #1e1e2e;
}

QObject,
QObject::separator,
QObject::handle,
QObject::tab-bar,
QObject::tab,
QObject::section {
  font-family: "Segoe UI", Frutiger, "Frutiger Linotype", "Dejavu Sans", "Helvetica Neue", "Microsoft YaHei UI", "PingFang SC", "Noto Sans SC", Arial, sans-serif;
  font-size: 10pt;
  margin: 0;
  padding: 0;
  outline: 0;
  border: 0;
  selection-background-color: rgba(147, 153, 178, 0.22);
  selection-color: #cdd6f4;
  alternate-background-color: transparent;
  color: #cdd6f4;
  border-radius: 2px;
}

QMainWindow,
QDockWidget {
